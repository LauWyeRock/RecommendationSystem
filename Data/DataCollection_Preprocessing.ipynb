    "# adding song features\n",
    "unique_songs = pd.read_csv('unique_songs.csv',  encoding='utf-8', dtype=str, na_values = [], na_filter = False)\n",
    "# unique_songs.isna().sum()\n",
    "songs = unique_songs.to_dict(orient = 'records')\n",
    "print(len(songs))\n",
    "# 335205\n",
    "\n",
    "\n",
    "base_url = 'http://ws.audioscrobbler.com/2.0/'\n",
    "lastfm_api_keys = [LASTFM_API_KEY, \"eba809a07b73a914631d5ad4498b5b65\", \"97d5a64d5ba4a8bc580b752ceff3b87f\", \"e246b3afd5a75f1cd05e3803aa7ecb36\", \"3a447e54a0ebc4ef9e2278590c66e4e4\"]\n",
    "\n",
    "def lastfm_get_parallel(api_key, payload):\n",
    "    headers = {'user-agent': 'DataCollectorBot'}\n",
    "    payload['api_key'] = api_key\n",
    "    payload['format'] = 'json'\n",
    "    response = requests.get(base_url, headers=headers, params=payload)\n",
    "    return response.json()\n",
    "\n",
    "def get_song_details_helper(api_key, track_name, artist_name):\n",
    "    payload = {\n",
    "        'method': 'track.getInfo',\n",
    "        'artist': artist_name,\n",
    "        'track': track_name,\n",
    "    }\n",
    "    json_response = lastfm_get_parallel(api_key, payload)\n",
    "    return json_response.get('track', {})\n",
    "\n",
    "def get_song_details_in_parallel(songs):\n",
    "    results = {}\n",
    "    with ThreadPoolExecutor(max_workers=len(lastfm_api_keys)) as executor:\n",
    "        future_to_song = {\n",
    "            executor.submit(get_song_details_helper, lastfm_api_keys[i % len(lastfm_api_keys)], song['track_name'], song['artist_name']): song\n",
    "            for i, song in enumerate(songs)\n",
    "        }\n",
    "        for future in as_completed(future_to_song):\n",
    "            song = future_to_song[future]\n",
    "            try:\n",
    "                details = future.result()\n",
    "                results[(song['track_name'], song['artist_name'])] = details\n",
    "                print(f\"fetching details for {song['track_name']}\")                \n",
    "            except Exception as e:\n",
    "                print(f\"Error fetching details for {song['track_name']}: {e}\")\n",
    "    return results\n"
   ]
