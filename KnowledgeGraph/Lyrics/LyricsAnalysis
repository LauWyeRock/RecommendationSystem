import lyricsgenius

genius = lyricsgenius.Genius("YOUR_GENIUS_ACCESS_TOKEN_HERE")

def get_lyrics(song_title, artist_name):
    song = genius.search_song(song_title, artist_name)
    if song:
        return song.lyrics
    return None

# Example usage
lyrics = get_lyrics("Shape of You", "Ed Sheeran")
print(lyrics)